git pull origin main


//push commands
git push
ssh-keygen -t rsa b 4094 -C "kazibaseema04@gmail.com"

//add generated pub key
git status
git add gitkey.pub
git status

//now push it
git push -u origin master
git commit -m 'have don push'

//git log
git log

//find by name
git log --online 'gitcommand.docx'

